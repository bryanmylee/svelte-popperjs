{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "emitDeclarationOnly": true,
    "declaration": true,
    "outFile": "dist/index.d.ts",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "**/*.test.ts"
  ]
}

