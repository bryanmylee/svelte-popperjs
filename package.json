{
	"name": "svelte-popperjs",
	"version": "1.3.2",
	"description": "Popper for Svelte with actions, no wrapper components required!",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"prepare": "rollup -c && tsc",
		"pretest": "eslint --ignore-path .gitignore .",
		"test": "vitest run --coverage",
		"test:only": "vitest run",
		"test:watch": "vitest",
		"format": "prettier --plugin-search-dir=. --write --ignore-path .gitignore ."
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/bryanmylee/svelte-popperjs.git"
	},
	"keywords": [
		"svelte",
		"sveltejs",
		"popper",
		"popperjs",
		"tooltip",
		"positioning"
	],
	"author": "Bryan Lee",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/bryanmylee/svelte-popperjs/issues"
	},
	"homepage": "https://github.com/bryanmylee/svelte-popperjs#readme",
	"peerDependencies": {
		"@popperjs/core": ">=2",
		"svelte": ">=3"
	},
	"devDependencies": {
		"@rollup/plugin-typescript": "^5.0.2",
		"@sveltejs/vite-plugin-svelte": "^1.0.2",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.1",
		"@typescript-eslint/eslint-plugin": "^4.11.0",
		"@typescript-eslint/parser": "^4.11.0",
		"@vitest/coverage-c8": "^0.22.1",
		"eslint": "^7.5.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jsdom": "^20.0.0",
		"prettier": "^2.5.1",
		"rollup": "^2.23.0",
		"tslib": "^2.0.0",
		"typescript": "^4.8.2",
		"vitest": "^0.22.1"
	}
}
